{
    "name": "odhiamboatieno/laravel-entrust",
    "description": "This package provides a flexible solution to add ACL to Laravel",
    "keywords": [
        "acl",
        "authorization",
        "php",
        "laravel",
        "entrust",
        "roles",
        "permissions",
        "laravel-entrust"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Odhiambo",
            "email": "odhiambosatieno@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/config": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10|^10.5",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Odhiambo\\LaravelEntrust\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Odhiambo\\LaravelEntrust\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Odhiambo\\LaravelEntrust\\LaravelEntrustServiceProvider"
            ],
            "aliases": {
                "LaravelEntrust": "Odhiambo\\LaravelEntrust\\Facades\\LaravelEntrustFacade"
            }
        }
    }
}
